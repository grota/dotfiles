{
  "LOG": true,
  "LOG_LEVEL": "debug",
  "CLAUDE_PATH": "",
  "HOST": "127.0.0.1",
  "PORT": 3456,
  "APIKEY": "",
  "API_TIMEOUT_MS": "600000",
  "PROXY_URL": "",
  "transformers": [],
  "Providers": [
    {
      "name": "openai",
      "api_base_url": "https://api.openai.com/v1/chat/completions",
      "api_key": "$OPENAI_API_KEY",
      "models": [
        "gpt-5",
        "gpt-5-mini",
        "gpt-5-nano",
        "gpt-4.1",
        "gpt-4.1-mini",
        "o3-mini",
        "o3",
        "gpt-4o",
        "gpt-4o-mini"
      ],
      "transformer": {
        "use": [
          "maxcompletiontokens"
        ]
      }
    },
    {
      "name": "openrouter",
      "api_base_url": "https://openrouter.ai/api/v1/chat/completions",
      "api_key": "$OPENROUTER_API_KEY",
      "models": [
        "@preset/kimi-k2-on-groq",
        "qwen/qwen3-coder"
      ],
      "transformer": {
        "qwen/qwen3-coder": {
          "use": [
            [
              "openrouter",
              {
                "provider": {
                  "only": [
                    "cerebras"
                  ]
                }
              }
            ]
          ]
        },
        "use": [
          "openrouter",
          [
            "maxtoken",
            {
              "max_tokens": 0
            }
          ]
        ]
      }
    },
    {
      "name": "gemini",
      "api_base_url": "https://generativelanguage.googleapis.com/v1beta/models/",
      "api_key": "$GEMINI_API_KEY",
      "models": [
        "gemini-2.5-flash",
        "gemini-2.5-pro"
      ],
      "transformer": {
        "use": [
          "gemini"
        ]
      }
    }
  ],
  "StatusLine": {
    "enabled": false,
    "currentStyle": "default",
    "default": {
      "modules": [
        {
          "type": "model",
          "icon": "ðŸ¤–",
          "text": "{{model}}",
          "color": "bright_yellow"
        }
      ]
    },
    "powerline": {
      "modules": [
        {
          "type": "model",
          "icon": "ðŸ¤–",
          "text": "{{model}}",
          "color": "bright_yellow"
        }
      ]
    }
  },
  "Router": {
    "default": "openrouter,qwen/qwen3-coder",
    "background": "openai,gpt-5-mini",
    "think": "openrouter,qwen/qwen3-coder",
    "longContext": "openrouter,qwen/qwen3-coder",
    "longContextThreshold": 60000,
    "webSearch": "gemini,gemini-2.5-flash",
    "image": "gemini,gemini-2.5-flash"
  },
  "CUSTOM_ROUTER_PATH": ""
}
